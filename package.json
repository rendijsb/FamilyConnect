{
  "name": "FamilyConnectMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "dev": "node scripts/start-dev.js",
    "setup": "npm install && cd backend && npm install && cd .. && npm run db:generate",
    "setup:android": "npm run android",
    "setup:ios": "cd ios && bundle install && bundle exec pod install && cd .. && npm run ios",
    "backend": "cd backend && npm run dev",
    "db:studio": "cd backend && npm run db:studio",
    "db:migrate": "cd backend && npm run db:migrate",
    "db:generate": "cd backend && npm run db:generate",
    "clean": "cd android && ./gradlew clean && cd .. && rm -rf ios/build",
    "clean:metro": "npx react-native start --reset-cache",
    "clean:all": "rm -rf node_modules && rm -rf backend/node_modules && npm run clean && npm install && cd backend && npm install",
    "postinstall": "cd ios && bundle install && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.24",
    "@reduxjs/toolkit": "^2.8.2",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-image-picker": "^8.2.1",
    "react-native-maps": "^1.25.0",
    "react-native-permissions": "^5.4.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-vector-icons": "^10.3.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}